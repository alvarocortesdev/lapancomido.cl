{
  "name": "@lapancomido/database",
  "version": "1.0.0",
  "main": "./src/index.cjs",
  "types": "./src/index.d.ts",
  "exports": {
    ".": {
      "require": "./src/index.cjs",
      "import": "./src/index.mjs"
    },
    "./client": {
      "require": "./src/client.cjs",
      "import": "./src/client.mjs"
    }
  },
  "scripts": {
    "generate": "prisma generate",
    "db:pull": "prisma db pull",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "postinstall": "prisma generate || true",
    "build": "echo 'No build step for database package'",
    "lint": "eslint src",
    "clean": "rm -rf src/generated"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^6.3.0",
    "@prisma/client": "^6.3.0",
    "pg": "^8.13.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/pg": "^8.11.0",
    "dotenv": "^16.4.7",
    "prisma": "^6.3.0",
    "typescript": "^5.3.0"
  }
}
