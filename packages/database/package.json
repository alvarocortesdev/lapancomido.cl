    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/pg": "^8.16.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "globals": "^15.15.0",
    "typescript-eslint": "^8.54.0"
  }
